<script language="javascript">
// Current time ==========================================
setInterval('dispDate()', 200);
function dispDate(){

	var day="";
	var month="";
	var myweekday="";
	var year="";
	newdate = new Date();
	mydate = new Date();
	dston =  new Date('April 4, 1999 2:59:59');
	dstoff = new Date('october 31, 1999 2:59:59');
	var myzone = newdate.getTimezoneOffset();
	newtime=newdate.getTime();
	
	var zonea = -8;  // references your time zone. positive is west!
	
	dst = "香港時間 HKT";
	
	var newzone = (zonea*60*60*1000);
	newtimea = newtime+(myzone*60*1000)-newzone;
	mydate.setTime(newtimea);
	myweekday = mydate.getDay();
	mymonth = mydate.getMonth();
	myday= mydate.getDate();
	myyear= mydate.getYear();
	year = myyear;
	
	if (year < 2000)
	year = year + 1900;
	myhours = mydate.getHours();
	if (myhours < 10){myhours = "0" + myhours;};
	myminutes = mydate.getMinutes();
	if (myminutes < 10){mytime = ":0" + myminutes;}else{mytime = ":" + myminutes;};
	myseconds = mydate.getSeconds();
	if (myseconds < 10){mytime += ":0" + myseconds;}else{mytime += ":" + myseconds;};
	
	arday = new Array("星期日 Sun ","星期一 Mon","星期二 Tue","星期三 Wed","星期四 Thu","星期五 Fri","星期六 Sat")
	armonth = new Array("Jan ","Feb ","Mar ","Apr ","May ","Jun ","Jul ","Aug ","Sep ", "Oct ","Nov ","Dec ")
	// rename locale as needed.
	
	var time = ("時間 Time: " + year + "/" + (mymonth+1) + "/" + myday + " " + arday[myweekday] + " " + myhours + mytime+ ", " + dst);

  	document.getElementById('dt').innerHTML = time;
}
// Current time ends =====================================
</script>
