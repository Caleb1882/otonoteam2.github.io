<script type="text/javascript">
function ajaxFunction(lat,longi,tz,height,dst,ecldate)
{
var xmlHttp;
try
  {
  // Firefox, Opera 8.0+, Safari
  xmlHttp=new XMLHttpRequest();
  }
catch (e)
  {
  // Internet Explorer
  try
    {
    xmlHttp=new ActiveXObject("Msxml2.XMLHTTP");
    }
  catch (e)
    {
    try
      {
      xmlHttp=new ActiveXObject("Microsoft.XMLHTTP");
      }
    catch (e)
      {
      alert("Your browser does not support AJAX!");
      return false;
      }
    }
  }
  xmlHttp.onreadystatechange=function()
    {
    if(xmlHttp.readyState==4)
      {
      document.getElementById("txtHint").innerHTML=xmlHttp.responseText;
      }
    }
  var longlink = "run.php?ecldate="+ecldate+"&lat="+lat+"&long="+longi+"&tz="+tz+"&height="+height+"&dst="+dst;
  xmlHttp.open("GET",longlink,true);
  xmlHttp.send(null);
}

function subroutine(){
	var lat = document.getElementById("lat").value;
	var longi = document.getElementById("longi").value;
	var tz = document.getElementById("tz").value;	
	var height = document.getElementById("height").value;
	var ecldate = document.eclform.ecldate.value;
	if (document.eclform.dst[0].checked){var dst = 0;}else{var dst = 1;}
	ajaxFunction(lat,longi,tz,height,dst,ecldate);
}

//http://www.rgagnon.com/jsdetails/js-0066.html
function openChild(file,window,size) {
	if (size == 1){
    	childWindow=open(file,window,'resizable=no,width=700,height=200,status=no,location=no');
	} else if (size == 2){
    	childWindow=open(file,window,'resizable=no,width=800,height=200,status=no,location=no');
	} else if (size == 3){
    	childWindow=open(file,window,'resizable=no,width=600,height=120,status=no,location=no');
	} 
	
    if (childWindow.opener == null) childWindow.opener = self;
}
</script>
</head>

